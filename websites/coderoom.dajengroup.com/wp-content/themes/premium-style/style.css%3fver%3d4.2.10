/**************** Premium Style social icon CSS ****************/
.site-social {
	float: right;
	padding: 10px 7px 0 5px;
	text-align: right;
	z-index: -10000000;
	vertical-align:baseline;
}
.site-social a { 
	text-decoration: none; 
}
.site-social a:hover { 
	text-decoration: none; 
}
.site-social span {
    display: inline;
    float: left;
    margin-right: 10px;
}
.site-social span a {
    background: url("images/social_icons.png") no-repeat scroll 0 0 rgba(0, 0, 0, 0);
    display: block;
    height: 32px;
    text-indent: -9999px;
    transition: all 0.2s ease 0s;
    width: 32px;
	text-indent: -9000em;
}
