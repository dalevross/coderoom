	.main-navigation li {
		display: inline-block;
		text-decoration: none;
		font-size: 11px;
    	font-weight: bold;
	}
	.main-navigation li a {
		color: #FFFFFF;
		white-space: nowrap;
		line-height: 36px; 
	}
	.main-navigation li a:hover {
		background: url(images/arrow-up.png) no-repeat center 32px;
		display: block;
		line-height: 36px; 		
		padding: 0px 15px !important; 
		-moz-border-radius: 0; 
		-webkit-border-radius: 0;
		border-radius: 0; 
		-webkit-box-shadow: none; 
		-moz-box-shadow: none; 
		box-shadow: none;
		color:#FFFFFF;
		background-color:#0074a2;
	}
	.main-navigation li {
		margin: 0 5px 0 0;
		position: relative;
	}
	.main-navigation li ul {
		display: none;
		margin: 0;
		padding: 0;
		position: absolute;
		top: 100%;
		z-index: 100;
	}
	.main-navigation li ul ul {
		top: 0;
		left: 100%;
	}
